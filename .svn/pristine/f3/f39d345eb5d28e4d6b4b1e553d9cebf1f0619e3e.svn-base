.center_btn {text-align: center;}
.center_btn_a {display:inline-block;}
.center_btn_a a {margin: 10px 10px 0 0;}
.blue_arrow {width:100%; height:2px; background:#41a4ff;}


/* 广告主数据整合 */
.account_list { border:#ddd solid 1px; text-align:center;}
.account_list td { padding:5px 10px;}
.account_list thead td { color:#eb6100;}
.account_list em { padding:0 5px;}
.cancel_save { float:right; margin:10px 0 10px 10px;}
.prevent_td {white-space:nowrap;}


/* 账户操作设置 */
.people_set_form {position:relative;}
.people_set_ul {float:left; margin:10px 0 0 140px;}
.people_set_ul li {float:left; margin-right:35px; line-height:30px;}
.people_set_area { height:50px; border:1px #ddd solid; background-color:#EBEDF5; position:relative;}
.set_name { width:90px; font-size:14px; color:#444654; line-height:49px; margin-left:20px; position:absolute; top:0; left:0; text-align:right;}
.long_btn .btn { width:288px;}
.operation_people {}
.operation_people_list { width:49.3%; float:left;}
.operation_people_area {margin:0 auto; width:67%;}
.area_title { background:#f4f4f4; padding:0 20px;}
.area_title p { float:left; line-height:40px;}
.border_rl { border-left:#ddd solid 1px;}
.checkbox_title { position:relative; text-align:left; padding:0 15px;}
.checkbox_title i { display:block; width:0px; height:0px; position:absolute; left:0; top:11px; border-top:8px #999 solid; border-left:4px transparent solid; border-right:4px transparent solid; border-bottom:0px;}
.checkbox_title i.shut_tb { display:block; width:0px; height:0px; position:absolute; left:0; top:11px; border-left:8px #999 solid; border-top:4px transparent solid; border-bottom:4px transparent solid; border-right:0px;}
.operation_people_area input { float:left; vertical-align:middle; margin:9px 5px 0 5px;}
.operation_people_area ul { text-align:left;}
.first_ul p { cursor:pointer;}
.second_ul { margin-left:40px;}
.second_ul span { float:left;}
.second_ul li { margin-left:33px;}
.fourth_ul { margin-left:40px;}
.fourth_ul span { float:left;}
.close_tb { width:16px; height:16px; float:left; cursor:pointer; margin:8px 0 0 15px; display:block; background:url(../images/menu_icon.png) no-repeat; background-position:-68px -66px;}
.close_tb:hover { background-position:-89px -66px;}


/* 广告主管理 */
.add_account label { float:left; width:133px; text-align:right; margin-right:20px;}
.add_account input.text { width:300px;}
.add_account button { width:300px;}
.add_account p { float:left; margin-right:20px;}
.add_account p input.radio { float:left; vertical-align:middle; float: left; margin: 9px 5px 0px;}
.add_account_list { padding:0 0 20px 200px; border-bottom:#ddd solid 1px;}
.add_account_title { font-size:14px;}


/* 游戏标签管理 */
.box_title p { float:left; line-height:49px; padding-left:50px;}
.box_title p em { margin-right:30px;}
.game_tally { border-top:#ddd solid 1px; border-left:#ddd solid 1px;}
.game_tally ul { width:50%; float:left; border-right:#ddd solid 1px; border-bottom:#ddd solid 1px;}
.game_tally ul li { float:left; width:33.3%; text-align:center;}
.add_account textarea { width:630px; height:110px;}
.bigtext_area {}
.bigtext_area ul { width:630px; float:left; height:190px; border:#ddd solid 1px; padding:20px;}
.bigtext_area li { float:left; background:#eee; border-radius:3px; margin-right:10px; padding:0 7px;}
.bigtext_area span { float:left;}
.label_relevance { overflow:hidden; height:110px;}

/* 关键词嵌套排查结果 */
.choose_time { margin-right:0;}
.padding_top_search { padding:0 250px; margin-bottom:20px;}


.input-group {
    margin-right: 10px;
    position: relative;
    display: table;
    float:left;
    border-collapse: separate;
}
