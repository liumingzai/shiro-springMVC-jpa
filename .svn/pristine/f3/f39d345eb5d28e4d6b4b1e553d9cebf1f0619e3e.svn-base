.common_note { width:16px; height:16px; display:block; float:right; margin:3px 5px 0 0; background:url(../images/menu_icon.png) 92px 55px;}
.small_common_note { width:11px; height:11px; display:block; float:right; margin:5px 5px 0 0; background:url(../images/menu_icon.png) 71px 53px;}
.pos_popup {position:relative;}
.recommend_table p {width:213px; overflow:hidden; word-wrap:break-word; font-size:12px; z-index:1000; line-height:18px; font-weight:normal; color:#333; display:none; border:#ddd solid 1px; background:#e9fbed; padding:5px; border-radius:2px; position:absolute; left:-10px; top:20px;}
.recommend_table p:hover {color:#333; word-wrap:break-word;}
.note_margin {margin:18px 0 0 10px;}

/* 用户管理 */
.add_small_margin {padding: 0 0 20px 100px; border-bottom: #ddd solid 1px;}


/* 账户列表 */
.tablelist {border-collapse: collapse;}
table {
                word-wrap: break-word;
                table-layout: fixed;
            }
.popup_tr {text-align:center;}
.popup_text_center {display:inline-block; text-align:left;}

.right_unfold_tb {width:8px; height:200px; position:fixed; top:30%; left:-20px; z-index:1000; background:url(../images/ss.png); cursor:pointer;}