@charset "utf-8";
/* 初始设置 */
body {position:relative; font-size:12px; line-height:30px; color:#666; background-color:#fff; font-family:"微软雅黑",arial,"宋体",sans-serif normal;}

body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,hr,button,article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section { padding:0px; margin:0px; -webkit-box-sizing:border-box; -moz-box-sizing:border-box; box-sizing:border-box;}
li { list-style:none;}
em,i { font-style:normal;}
img { border:0px;}
label { font-weight:normal; margin:0px;}
textarea {resize: none;}
a { color:#666; cursor:pointer; transition:background-color 0.2s ease-out , color 0.2s ease-out 0s;}
a:hover { color:#42A4FF; text-decoration:none;}
table tr {table-layout:fixed;}
.none { display:none;}
@font-face { font-family:arial; src:url('../fonts/arial.ttf');}
.arial { font-family:arial;}

